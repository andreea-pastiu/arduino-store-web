@model OrderModel
@{
    <table>
        <tr>
            <th>OrderId</th>
            <th>Total</th>
        </tr>
        @for (int i = 0; i < Model.Orders.Count; i++)
        {
            Order order = Model.Orders[i];
            <tr>
                <td>@order.OrderId</td>
                <td>@order.Total</td>
            </tr>
        }
    </table>
}
